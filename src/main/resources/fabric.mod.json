{
  "schemaVersion": 1,
  "id": "keywizard",
  "version": "${version}",

  "name": "Keyboard Wizard",
  "description": "Keyboard Wizard is simple that makes finding and fixing key bind conflicts a lot easier",
  "authors": [
    "MrNerdy42"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/keyboard-wizard",
    "sources": "https://github.com/MrNerdy42/keywizard-fabric"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "mrnerdy42.keywizard.KeyWizard"
    ]
  },
  "mixins": [
    "keywizard.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16",
    "fabric-key-binding-api-v1": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
